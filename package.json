    "sendmail": "^1.6.1",
    "sequelize": "^5.22.3",
    "slick-carousel": "^1.8.1",
    "suncalc": "^1.8.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
